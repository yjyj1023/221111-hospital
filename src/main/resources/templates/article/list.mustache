{{>header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">목록</th>
        <th scope="col">제목</th>
        <th scope="col">내용</th>
    </tr>
    </thead>
    <tbody class="table-group-divider">
    {{#board}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/hospital/board/{{id}}" style="text-decoration:none">{{title}}</a></td>
            <td>{{contents}}</td>
        </tr>
    {{/board}}
    </tbody>
</table>
<a class='btn btn-primary btn-sm mb-2' href="/hospital/board/new">새 글 작성</a>
{{>footer}}